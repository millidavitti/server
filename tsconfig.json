{
	"compilerOptions": {
		"target": "ES2017",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"outDir": "dist",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedParameters": true,
		"baseUrl": ".",
		"paths": {
			"@db/*": ["src/db/*"],
			"@model/*": ["src/model/*"],
			"@routes/*": ["src/routes/*"]
		}
	},
	"exclude": ["node_modules"]
}
